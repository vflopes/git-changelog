{
  "name": "git-changelog",
  "version": "1.0.0",
  "description": "Smoothly create changelogs from commits",
  "main": "index.js",
  "scripts": {
    "build": "pkg --o ./dist/git-changelog --targets node10-linux-x64,node10-linux-x86,node10-win-x64,node10-win-x86,node10-macos-x64 --config ./package.json ./index.js"
  },
  "pkg":{
	  "assets": [
		"templates/**/*"
	]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vflopes/git-changelog.git"
  },
  "keywords": [
    "git",
    "commit",
    "changelog",
    "trigger"
  ],
  "author": "Victor Fran√ßa Lopes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vflopes/git-changelog/issues"
  },
  "homepage": "https://github.com/vflopes/git-changelog#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "dot": "^1.1.2",
    "inquirer": "^6.2.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.0.2"
  }
}
